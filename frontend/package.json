{
  "name": "stationhead-clone",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-icons": "^4.12.0",
    "react-scripts": "5.0.1",
    "socket.io-client": "^4.7.2"
  },
  "scripts": {
     "build-css": "tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.output.css && postcss /App.css -o /App.min.css",
    "watch-css": "tailwindcss build src/styles/tailwind.css -o src/styles/tailwind.output.css --watch",
    "prestart": "npm run build-css",
    "start": "react-scripts start",
    "prebuild": "npm run build-css",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0"
  }
}
